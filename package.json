{
  "name": "pogo-notifier-bot",
  "version": "0.1.0",
  "description": "Pokemon go bot that scrapes mappers and other apis for data to notify users of revelant information",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npm run generate && npm run deploy && tsc -p .",
    "dev": "nodemon src/index.ts",
